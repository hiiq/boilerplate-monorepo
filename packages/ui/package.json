    "react-axe": "^3.2.0",
    "sentry-files": "^1.0.1",
    "serve": "^11.3.0",
    "shelljs": "^0.8.3",
    "stylelint": "^10.1.0",
    "stylelint-config-recommended": "^2.2.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-order": "^3.0.1",
    "stylelint-prettier": "^1.1.1",
    "stylelint-processor-styled-components": "^1.8.0",
    "testdouble": "^3.12.4",
    "testdouble-jest": "^2.0.0",
    "update-dotenv": "^1.1.1"
  }
}
