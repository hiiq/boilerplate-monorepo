{
  "name": "@boilerplate-monorepo/service",
  "version": "0.0.1",
  "description": "Awesome project developed with TypeORM.",
  "license": "MIT",
  "engines": {
    "node": "^12.13.0",
    "yarn": "^1.19.1"
  },
  "scripts": {
    "build": "yarn && babel src -d dist",
    "migrate": "babel-node node_modules/.bin/knex",
    "migration:cli": "node \"../../node_modules/typeorm/cli.js\"",
    "migration:down": "yarn -s migrate migrate:down",
    "migration:gen": "yarn -s migrate migrate:make",
    "migration:latest": "yarn -s migrate migrate:latest",
    "migration:show": "yarn -s migrate migrate:list",
    "migration:up": "yarn -s migrate migration:up",
    "publish:service": "apollo service:push --tag local --endpoint=http://localhost:4000/graphql",
    "start": "yarn -s watch",
    "start:prod": "node -r dotenv/config dist",
    "watch": "nodemon --delay 1 --signal SIGHUP --exec babel-node -r dotenv/config src"
  },
  "dependencies": {
    "@boilerplate-monorepo/common": "^1.0.1",
    "apollo-server-cache-redis": "^1.1.5",
    "apollo-server-express": "^2.9.15",
    "bcryptjs": "^2.4.3",
    "bookshelf": "^1.1.0",
    "bookshelf-case-converter-plugin": "^1.0.0",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "cuid": "^2.1.8",
    "date-fns": "^2.8.1",
    "debug": "^3.2.6",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "graphql": "^14.5.8",
    "jsonwebtoken": "^8.5.1",
    "knex": "^0.20.13",
    "merge-graphql-schemas": "^1.7.6",
    "ms": "^2.1.2",
    "pg": "^7.3.0",
    "pg-connection-string": "^2.2.0",
    "ramda": "^0.26.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/preset-env": "^7.5.5",
    "babel-jest": "^24.8.0",
    "babel-plugin-macros": "^2.6.1",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-plugin-transform-decorators": "^6.24.1",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-plugin-eslint-comments": "^3.1.2",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-import-helpers": "^1.0.2",
    "eslint-plugin-jest": "^22.15.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-ramda": "^2.5.1",
    "nodemon": "^2.0.2"
  }
}
