{
  "name": "@boilerplate-monorepo/ui",
  "version": "1.0.1",
  "private": true,
  "homepage": "https://n8io.github.io",
  "engines": {
    "node": "^14.2.0",
    "yarn": "^1.19.1"
  },
  "scripts": {
    "build": "craco build",
    "build:gh": "yarn -s node .prebuild && REACT_APP_PUBLIC_URL=/boilerplate-monorepo yarn -s build",
    "deploy": "gh-pages -d build",
    "deploy:gh": "REACT_APP_PUBLIC_URL=/boilerplate-monorepo yarn -s deploy",
    "prestart": "yarn -s start:pre",
    "serve": "serve -s -n build",
    "start": "craco start",
    "start:pre": "(cat ../../.ini/certs/cert.pem ../../.ini/certs/key.pem > ../../node_modules/webpack-dev-server/ssl/server.pem) || :",
    "start:prod": "yarn run -s serve -l 3000 --ssl-cert ../../.ini/certs/cert.pem --ssl-key ../../.ini/certs/key.pem",
    "start:prod:heroku": "yarn run -s serve",
    "test": "craco test --passWithNoTests",
    "test:ci": "JEST_JUNIT_OUTPUT_DIR=../../.tmp/test-results/jest/ui yarn -s test --ci --runInBand --reporters=default --reporters=jest-junit",
    "test:watch": "craco test --passWithNoTests --color"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "size-limit": [
    {
      "path": "build/static/**/*.{js,css,svg,woff,woff2,png,ttf,html}"
    }
  ],
  "dependencies": {
    "@apollo/client": "^3.0.0-beta.20",
    "@boilerplate-monorepo/common": "^1.0.1",
    "@boilerplate-monorepo/ui-common": "^1.0.1",
    "@craco/craco": "^5.2.4",
    "@hcaptcha/react-hcaptcha": "^0.1.7",
    "@sentry/browser": "^5.6.3",
    "@sentry/types": "^5.7.1",
    "apollo-link-error": "^1.1.12",
    "apollo-link-persisted-queries": "^0.2.2",
    "apollo-link-retry": "^2.2.15",
    "change-case": "^4.1.1",
    "cuid": "^2.1.8",
    "date-fns": "^2.12.0",
    "debug": "^4.1.1",
    "gravatar": "^1.8.0",
    "i18next": "^17.0.11",
    "jwt-decode": "^2.2.0",
    "logrocket": "^1.0.3",
    "logrocket-react": "^3.0.1",
    "polished": "^3.4.1",
    "prop-types": "^15.7.2",
    "ramda": "^0.27.0",
    "react": "^0.0.0-experimental-33c3af284",
    "react-dom": "^0.0.0-experimental-33c3af284",
    "react-focus-lock": "^2.0.5",
    "react-helmet": "^5.2.1",
    "react-hook-form": "^5.4.2",
    "react-hook-form-devtools": "^1.1.3",
    "react-i18next": "^10.12.0",
    "react-icons": "^3.8.0",
    "react-responsive": "^8.0.1",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.0.1",
    "react-sidebar": "^3.0.2",
    "react-spring": "^8.0.27",
    "react-toastify": "^5.5.0",
    "reakit": "^1.0.0-beta.4",
    "styled-components": "^4.3.2",
    "styled-media-query": "^2.1.2",
    "styled-reset-advanced": "^1.0.1",
    "styled-theming": "^2.2.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.7.0",
    "@testing-library/react": "^10.0.4",
    "@types/jwt-decode": "^2.2.1",
    "babel-plugin-add-react-displayname": "^0.0.5",
    "babel-plugin-jsx-remove-data-test-id": "^2.0.0",
    "babel-plugin-macros": "^2.6.1",
    "babel-plugin-styled-components": "^1.10.6",
    "dotenv": "^8.1.0",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-import-resolver-lerna": "^1.1.0",
    "eslint-plugin-cypress": "^2.6.1",
    "eslint-plugin-eslint-comments": "^3.1.2",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-import-helpers": "^1.0.2",
    "eslint-plugin-jest": "^23.1.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "gh-pages": "^2.1.1",
    "git-rev-sync": "^1.12.0",
