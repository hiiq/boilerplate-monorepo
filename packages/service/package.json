{
  "name": "@boilerplate-monorepo/service",
  "version": "0.0.1",
  "description": "Awesome project developed with TypeORM.",
  "license": "MIT",
  "engines": {
    "node": "^12.13.0",
    "yarn": "^1.19.1"
  },
  "scripts": {
    "build": "tsc",
    "migration:cli": "ts-node -r tsconfig-paths/register \"../../node_modules/typeorm/cli.js\"",
    "migration:down": "yarn -s migration:cli migration:revert",
    "migration:gen": "typeorm migration:create --name",
    "migration:show": "yarn -s migration:cli migration:show",
    "migration:up": "yarn -s migration:cli migration:run",
    "publish:service": "apollo service:push --tag local --endpoint=http://localhost:4000/graphql",
    "start": "nodemon --exec ts-node -r tsconfig-paths/register src/index.ts",
    "start:prod": "node -r ts-node/register -r tsconfig-paths/register dist/index.js dist"
  },
  "dependencies": {
    "apollo-server-cache-redis": "^1.1.5",
    "apollo-server-express": "^2.9.15",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "cuid": "^2.1.8",
    "date-fns": "^2.8.1",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "graphql": "^14.5.8",
    "jsonwebtoken": "^8.5.1",
    "ms": "^2.1.2",
    "pg": "^7.3.0",
    "ramda": "^0.26.1",
    "reflect-metadata": "^0.1.10",
    "ts-node": "^8.5.4",
    "tsconfig-paths": "^3.9.0",
    "type-graphql": "^0.17.6",
    "typeorm": "0.2.22",
    "typeorm-naming-strategies": "^1.1.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.6",
    "@types/cuid": "^1.3.0",
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.2",
    "@types/jsonwebtoken": "^8.3.5",
    "@types/ms": "^0.7.31",
    "@types/node": "^8.0.29",
    "@types/node-fetch": "^2.5.4",
    "@types/ramda": "^0.26.39",
    "apollo": "^2.21.2",
    "nodemon": "^2.0.2",
    "typescript": "^3.7.4"
  }
}
